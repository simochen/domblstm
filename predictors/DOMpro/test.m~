clear all
load pred.mat
load multi.mat
[trNum, precision, recall, ndo] = evaluate(ms_cnt, ms_poi, md_range, seq_cnt, seq_poi, range_mdf);

sub = find(seq_cnt <= 3);
order = 0:length(sub)-1;
s_cnt = ms_cnt(sub);
s_poi = sum(s_cnt(1:));
cnt = seq_cnt(sub);
poi = seq_poi(sub);
range_pred = [];
range_def = [];
for i = 1:length(sub)
    range_pred = [range_pred; md_range(s_poi(i): s_poi(i)+s_cnt(i)-1,:)];
    range_def = [range_def; range_mdf(poi: poi+cnt-1, :)];
end
[trNum, precision, recall, ndo] = evaluate(s_cnt, s_poi, range_pred, cnt, poi, range_def);